{
    "env": {
        "myDefaultIncludePath": ["${workspaceFolder}/src"]
    },
    "configurations": [
      {
        "name": "Local",
        "intelliSenseMode": "${default}",
        "includePath": ["${myDefaultIncludePath}"],
        "cStandard": "c11",
